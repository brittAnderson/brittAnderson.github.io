# Temporarily store uncommited changes
git stash

# Verify correct branch
git checkout develop

# Build new files
./dist-newstyle/build/x86_64-linux/ghc-8.8.4/brittAnderson-github-io-0.1.0.0/x/site/build/site/site clean
./dist-newstyle/build/x86_64-linux/ghc-8.8.4/brittAnderson-github-io-0.1.0.0/x/site/build/site/site build

# Get previous files
git fetch --all
git checkout -b master --track origin/master

# Overwrite existing files with new files
cp -a _site/. .

# Commit
git add -A
git commit -m "Publish."

# Push
git push origin master:master

# Restoration
git checkout develop
git branch -D master
git stash pop